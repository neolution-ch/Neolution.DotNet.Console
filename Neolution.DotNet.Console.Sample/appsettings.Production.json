{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "NLog": {
    "targets": {
      "file": {
        "type": "File",
        "layout": "${longdate:universalTime=true} ${NLNHelperLogLevel} ${logger}: ${message}",
        "fileName": "${basedir}/logs/${shortdate}.log",
        "keepFileOpen": "true"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "console, file"
      }
    ]
  }
}
